{
    "manifest_version": 2,
    "name": "Cowin Expert",
    "description": "Chrome Extension to speed up covid vaccine slot booking including captcha solving.",
    "version": "1.0.0",
    "content_scripts": [
        {
            "matches": [
                "https://selfregistration.cowin.gov.in/*"
            ],
            "js": [
                "jquery.min.js",
                "bootstrap.min.js",
                "model.js",
                "contentScript.js",
                "captcha-dispatch-event.js",
                "shortcut.js",
                "sessionexp.js"
            ],
            "css": [
                "bootstrap.min.css",
                "custom.css"
            ]
        }
    ],
    "icons": {
        "128": "icon.png"
    }
}
